@model darbuotoju_valdymos_sistema.Models.Workers
<div class="modal-content">
    <div class="modal-header">
        <button type="button" id="closebttn" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Modal title</h4>
    </div>
    <div class="modal-body">
        Search for task:<input type="text" id="searchTaskTerm" oninput="searchTaskInWorkerView()" name="searchTaskTerm" value=""><br/>
        <table class="table" id="userTable">
            @await Html.PartialAsync("_UserDataView", Model)
         </table>
            <th>
                Unassigned Tasks
            </th>
         <table class="table" id="excludedTable">
             
            @await Html.PartialAsync("_ExcludedTasks", Model.excludedTasks)
         </table>

    </div>
    <div class="modal-footer">
        <button type="button" id="closebttn" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
</div>
