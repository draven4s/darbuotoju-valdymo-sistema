@model darbuotoju_valdymos_sistema.Models.Workers
<div class="modal-content">
    <div class="modal-header">
        <button type="button" id="closebttn" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Create new worker</h4>
    </div>
    <div class="modal-body">
        Worker name:<input type="text" id="newWorkerName" name="newWorkerName" value=""><br>
        Worker surname:<input type="text" id="newWorkerLastname" name="newWorkerLastname" value=""><br>
        <button type="button" id="closebttn" data-dismiss="modal" onclick="SaveNewWorker()">Create</button>
    </div>
    <div class="modal-footer">
        <button type="button" id="closebttn" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
</div>
